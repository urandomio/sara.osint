        { name: "Centralops", url: "https://centralops.net/" },
        { name: "ahmia.fi", url: "http://ahmia.fi/" },
        { name: "Onion Search Engine", url: "https://onionengine.com/" },
        { name: "Tor.Link", url: "https://tor.link/" },
      ]}
    />

    <ToolSection
      title="People Search"
      columns={2}
      items={[
        { name: "Radaris", url: "https://radaris.com/" },
        { name: "Zaba Search", url: "https://zabasearch.com/" },
